
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">상세읽기 - @Model.NoteTitle</h1>
    </div>
</div>

<div class="row">
    <div class="col-lg-10">
        <div class="panel panel-default">
            <div class="panel-heading">
                <label>제목 : </label> @Model.NoteTitle
            </div>
            <div class="panel-body">
                @Html.Raw(Model.NoteContents)
            </div>
        </div>
        <div class="panel-footer text-right">
            <a class="btn btn-primary" asp-controller="Note" asp-action="Index">목록</a>
        </div>
        <div class="panel-footer text-right">
            <a class="btn btn-secondary" asp-controller="Note" asp-action="Edit">수정</a>
        </div>
    </div>
</div>